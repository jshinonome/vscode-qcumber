{
  "name": "vscode-qcumber",
  "displayName": "qCumber",
  "author": {
    "name": "Jo Shinonome",
    "email": "jshinonome@gmail.com"
  },
  "description": "qCumber(quke) file syntax highlight support",
  "version": "1.0.0",
  "publisher": "jshinonome",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jshinonome/vscode-qcumber.git"
  },
  "icon": "icon.png",
  "contributes": {
    "languages": [
      {
        "id": "quke",
        "extensions": [
          ".quke"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./icon.png",
          "dark": "./icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "quke",
        "scopeName": "source.quke",
        "path": "./syntaxes/quke.tmLanguage.json"
      }
    ]
  }
}
